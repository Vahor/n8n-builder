{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"noEmit": true,
		"skipLibCheck": true,
		"declaration": true,
		// TODO: ../../node_modules/@n8n/tournament/src/FunctionEvaluator.ts:10:4 - error TS2322: Type 'Function | undefined' is not assignable to type 'Function'.
		"strictNullChecks": false,
		"paths": {
			"n8n-nodes-base/*": ["./vendor/n8n/packages/nodes-base/*"],
			"generated/nodes/*": ["./generated/nodes/*"]
		}
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules", "vendor"]
}
